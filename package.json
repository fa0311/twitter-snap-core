{
  "name": "twitter-snap-core",
  "version": "0.0.33",
  "description": "Create beautiful Tweet images fast",
  "keywords": [
    "twitter",
    "screenshots"
  ],
  "homepage": "https://github.com/fa0311/twitter-snap-core",
  "bugs": "https://github.com/fa0311/twitter-snap/issues",
  "repository": "fa0311/twitter-snap-core",
  "license": "MIT",
  "author": "ふぁ @fa0311",
  "scripts": {
    "example": "npm run example:theme && npm run example:video && npm run example:text",
    "example:text": "npx tsx example/text.ts",
    "example:video": "npx tsx example/video.ts",
    "example:theme": "npx tsx example/theme.ts",
    "dev:style": "shx rm -rf .next && next dev",
    "start": "node dist/cli.js",
    "build": "npx tsc -p tsconfig.build.json"
  },
  "bin": {
    "twitter-snap": "dist/cli.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "graphemesplit": "^2.4.4",
    "react": "^18.2.0",
    "twitter-openapi-typescript": "^0.0.33",
    "twitter-openapi-typescript-generated": "*"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vercel/og": "^0.6.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "next": "14.1.3",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "typescript": "^5"
  }
}
